{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ES2020", "DOM"],
    "allowJs": false,
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "types": ["node", "obsidian"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}

